<%= simple_form_for @laboratory, :html => { :role => 'form' } do |f| %>
	<div class="form-group">
	  <%= f.input :name, input_html: { class: 'form-control' } %>
	</div>
	<div class="form-group">  
	  <%= f.input :subdomain, input_html: { class: 'form-control'} %>
	</div>
    <%= f.input :custom_forms, as: :hidden %>
    <div class="custom_forms_holder"></div>

  <%= f.button :submit, :class => 'btn btn-primary' %>
  <br><br>
<% end %>
