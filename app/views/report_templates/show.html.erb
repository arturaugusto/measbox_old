<div class="page-header">
  <%= link_to report_templates_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_report_template_path(@report_template), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show report_template</h1>
</div>

<dl class="dl-horizontal">
  <dt>Laboratory:</dt>
  <dd><%= @report_template.laboratory_id %></dd>

  <dt>Name:</dt>
  <dd><%= @report_template.name %></dd>

  <dt>Value:</dt>
  <dd><%= @report_template.value %></dd>

</dl>
